{
  "name": "website",
  "version": "0.9.0",
  "private": true,
  "description": "Personal site built with React, Next.js, and Firebase",
  "bugs": "https://github.com/4cm4k1/website/issues",
  "repository": "https://github.com/4cm4k1/website",
  "license": "MIT",
  "author": "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>",
  "contributors": [
    "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>"
  ],
  "main": "index.js",
  "scripts": {
    "alias": "now alias -t $NOW_TOKEN",
    "build": "next build",
    "precommit": "pretty-quick --staged",
    "deploy": "now -t $NOW_TOKEN",
    "html": "html-minifier -c .html-minifier.json --input-dir build/web --output-dir build/web --file-ext html",
    "json": "html-minifier -c .html-minifier.json --input-dir build/web/assets --output-dir build/web/assets --file-ext json",
    "remark": "remark . -o && git add '*.md'",
    "sort": "sort-package-json && git add package.json",
    "start": "node index.js",
    "svg": "html-minifier -c .html-minifier.json --input-dir build/web --output-dir build/web --file-ext svg",
    "prettier": "prettier '{*,{lib,test,web}/**/*}.{css,js,json,md,scss}' --write",
    "prettier-watch": "onchange '{*,{lib,test,web}/**/*}.{css,js,json,md,scss}' -- prettier --write {{changed}}",
    "watch": "NODE_ENV=dev npm-run-all prettier-watch start",
    "xml": "html-minifier -c .html-minifier.json --input-dir build/web --output-dir build/web --file-ext xml"
  },
  "dependencies": {
    "@material/react-button": "^0.3.0",
    "@material/react-card": "^0.3.0",
    "@material/react-fab": "^0.3.0",
    "@material/react-floating-label": "^0.3.0",
    "@material/react-line-ripple": "^0.3.0",
    "@material/react-material-icon": "^0.3.0",
    "@material/react-notched-outline": "^0.3.0",
    "@material/react-ripple": "^0.3.0",
    "@material/react-text-field": "^0.3.0",
    "@material/react-top-app-bar": "^0.3.0",
    "firebase": "^5.3.0",
    "next": "^6.1.1-canary.2",
    "polka": "^0.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "shrink-ray-current": "^2.1.2"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.2",
    "@types/next": "^6.1.0",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "remark": "^9.0.0",
    "remark-lint": "^6.0.2",
    "remark-preset-github": "^0.0.8",
    "sort-package-json": "^1.15.0"
  },
  "now": {
    "alias": "anthony.codes",
    "engines": {
      "node": "^10.0.0"
    },
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "build",
      "index.js"
    ],
    "name": "anthony-codes",
    "public": true,
    "type": "npm"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  }
}
