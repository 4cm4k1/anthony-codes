{
  "name": "personal-website",
  "version": "0.9.3",
  "private": true,
  "description": "Personal website – made with Next.js, React, MDX, and Material Components – deployed on ZEIT Now",
  "bugs": "https://github.com/4cm4k1/personal-website/issues",
  "repository": "https://github.com/4cm4k1/personal-website",
  "license": "MIT",
  "author": "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>",
  "contributors": [
    "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>"
  ],
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "dev": "next",
    "dev:proxy": "run-p -r dev proxy",
    "gen-dev-certs": "mkcert localhost 127.0.0.1 ::1 && mv localhost+2.pem dev/cert.pem && mv localhost+2-key.pem dev/key.pem",
    "now-build": "next build",
    "start": "next start",
    "prettier": "prettier {*,**/*}.{babelrc,html,js,json,jsx,md,mdx,scss} --write",
    "proxy": "node dev/server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "next": "^8.0.5-canary.32",
    "react": "^16.9.0-alpha.0",
    "react-dom": "^16.9.0-alpha.0"
  },
  "devDependencies": {
    "@material/react-drawer": "^0.6.2",
    "@material/react-layout-grid": "^0.11.0",
    "@material/react-material-icon": "^0.11.0",
    "@material/react-top-app-bar": "^0.11.0",
    "@mdx-js/loader": "^1.0.0-rc.4",
    "@next/bundle-analyzer": "^8.0.5-canary.32",
    "@next/mdx": "^8.0.5-canary.32",
    "@rmwc/list": "^5.3.1",
    "@types/copy-webpack-plugin": "^4.4.3",
    "@types/fibers": "^0.0.30",
    "@types/http-proxy": "^1.17.0",
    "@types/next": "^8.0.3",
    "@types/node": "^11.13.4",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/prettier": "^1.16.1",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/sass": "^1.16.0",
    "@types/webpack": "^4.4.27",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@zeit/next-sass": "^1.0.2-canary.2",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "extract-css-chunks-webpack-plugin": "^4.3.0",
    "fibers": "^3.1.1",
    "http-proxy": "^1.17.0",
    "husky": "^1.3.1",
    "modern-normalize": "^0.5.0",
    "next-compose-plugins": "^2.1.1",
    "next-offline": "^3.3.8",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "renovate-config": {
    "assignees": [
      "4cm4k1"
    ],
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies"
    ],
    "labels": [
      "renovate"
    ]
  }
}
