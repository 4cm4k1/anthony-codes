{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "description": "Personal site built with Dart, Angular, and Firebase",
  "bugs": "https://github.com/4cm4k1/website/issues",
  "license": "MIT",
  "author": "Anthony Maki <4cm4k1@gmail.com> (https://anthony.codes)",
  "contributors": [
    "Anthony Maki <4cm4k1@gmail.com> (https://anthony.codes)"
  ],
  "repository": "https://github.com/4cm4k1/website",
  "scripts": {
    "alias": "now alias -t $NOW_TOKEN",
    "build": "pub build && webpack",
    "deploy": "now ./build/web -t $NOW_TOKEN",
    "lint": "remark readme.md -o && sort-package-json",
    "start": "concurrently -rk \"serve node_modules -nSt\" \"pub serve\""
  },
  "dependencies": {
    "firebase": "^4.8.1",
    "normalize.css": "^7.0.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-beta.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "remark": "^8.0.0",
    "remark-cli": "^4.0.0",
    "remark-lint": "^6.0.1",
    "remark-preset-github": "^0.0.7",
    "serve": "^6.4.3",
    "sort-package-json": "^1.7.1",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  }
}
