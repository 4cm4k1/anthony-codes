{
  "name": "website",
  "version": "0.9.0",
  "private": true,
  "description": "Personal site built with React, Next.js, and Material Components",
  "bugs": "https://github.com/4cm4k1/website/issues",
  "repository": "https://github.com/4cm4k1/website",
  "license": "MIT",
  "author": "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>",
  "contributors": [
    "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>"
  ],
  "scripts": {
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "build": "next build",
    "dev": "next",
    "now-build": "next build --lambdas",
    "prettier": "prettier '{*,{components,lib,mdx,pages}/**/*}.{babelrc,css,js,json,jsx,md,mdx}' --write",
    "start": "next start"
  },
  "dependencies": {
    "@material/react-drawer": "^0.6.2",
    "@material/react-layout-grid": "^0.7.0",
    "@material/react-material-icon": "^0.7.0",
    "@material/react-top-app-bar": "^0.7.0",
    "@mdx-js/loader": "^0.16.5",
    "@mdx-js/mdx": "^0.16.5",
    "@rmwc/list": "^3.0.11",
    "@zeit/next-bundle-analyzer": "^0.1.3-canary.1",
    "@zeit/next-mdx": "^1.2.1-canary.0",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "next": "^7.0.2-canary.27",
    "next-compose-plugins": "^2.1.1",
    "next-offline": "^3.2.1",
    "next-optimized-images": "^1.4.1",
    "next-server": "^7.0.2-canary.27",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@types/material-components-web": "^0.35.0",
    "@types/next": "^7.0.5",
    "@types/next-server": "^7.0.2",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "husky": "^1.2.0",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0"
  },
  "renovate-config": {
    "assignees": [
      "4cm4k1"
    ],
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies"
    ],
    "labels": [
      "renovate"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
