{
  "name": "personal-website",
  "version": "0.9.3",
  "private": true,
  "description": "Personal website – made with Next.js, React, MDX, and Material Components – deployed on ZEIT Now",
  "bugs": "https://github.com/4cm4k1/personal-website/issues",
  "repository": "https://github.com/4cm4k1/personal-website",
  "license": "MIT",
  "author": "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>",
  "contributors": [
    "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>"
  ],
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "dev": "next",
    "dev:proxy": "run-p -r dev proxy",
    "gen-dev-certs": "mkcert localhost 127.0.0.1 ::1 && mv localhost+2.pem dev/cert.pem && mv localhost+2-key.pem dev/key.pem",
    "now-build": "next build",
    "start": "next start",
    "prettier": "prettier {*,**/*}.{babelrc,html,js,json,jsx,md,mdx,scss} --write",
    "proxy": "node scripts/server.js",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "next": "^9.0.4-canary.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@material/react-drawer": "^0.15.0",
    "@material/react-layout-grid": "^0.15.0",
    "@material/react-material-icon": "^0.15.0",
    "@material/react-top-app-bar": "^0.15.0",
    "@mdx-js/loader": "^1.2.2",
    "@next/bundle-analyzer": "^9.0.4-canary.3",
    "@next/mdx": "^9.0.4-canary.3",
    "@rmwc/list": "^5.6.0",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@zeit/next-sass": "^1.0.2-canary.2",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "husky": "^3.0.3",
    "modern-normalize": "^0.5.0",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^4.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sass": "^1.22.9",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "sass-loader": "^7.2.0"
  },
  "engines": {
    "node": ">=10.x"
  },
  "renovate-config": {
    "assignees": [
      "4cm4k1"
    ],
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies"
    ],
    "labels": [
      "renovate"
    ]
  }
}
