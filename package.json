  "devDependencies": {
    "@firebase/app-types": "^0.3.2",
    "@types/next": "^7.0.3",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "debug": "^4.1.0",
    "husky": "^1.1.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "webpack": "^4.21.0"
