{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "description": "Personal site built with Dart, Angular, and Firebase",
  "bugs": "https://github.com/4cm4k1/website/issues",
  "license": "MIT",
  "author": "Anthony Maki <4cm4k1@gmail.com> (https://anthony.codes)",
  "contributors": ["Anthony Maki <4cm4k1@gmail.com> (https://anthony.codes)"],
  "main": "index.js",
  "repository": "https://github.com/4cm4k1/website",
  "scripts": {
    "alias": "now alias -A ./now.json -t $NOW_TOKEN",
    "build": "pub run build_runner build --output=build && webpack",
    "deploy": "now -t $NOW_TOKEN",
    "lint": "remark readme.md -o && sort-package-json",
    "now-build": "echo 'Starting app!'",
    "precommit": "pretty-quick --staged",
    "start": "node index.js",
    "watch":
      "concurrently \"onchange '**/*.{css,js,json,md,scss}' -- prettier --write {{changed}}\" \"pub run build_runner watch --output=build\" \"webpack\" \"NODE_ENV=dev yarn run start\""
  },
  "dependencies": {
    "shrink-ray": "^0.1.3",
    "superstatic": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.8",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "^4.8.2",
    "husky": "^0.14.3",
    "node-env-webpack-plugin": "^1.1.0",
    "normalize.css": "^7.0.0",
    "onchange": "^3.3.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.0",
    "remark": "^8.0.0",
    "remark-cli": "^4.0.0",
    "remark-lint": "^6.0.1",
    "remark-preset-github": "^0.0.7",
    "sort-package-json": "^1.7.1",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": "9.x.x"
  },
  "remarkConfig": {
    "plugins": ["preset-github"]
  }
}
