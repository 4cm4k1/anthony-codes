{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": [">1%", "not ie 11"],
          "useBuiltIns": "usage"
        },
        "styled-jsx": {
          "plugins": [
            [
              "styled-jsx-plugin-sass",
              {
                "sassOptions": {
                  "includePaths": ["./node_modules/"]
                }
              }
            ]
          ]
        }
      }
    ]
  ]
}
